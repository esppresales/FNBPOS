<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlidingLayer">
        <attr format="boolean" name="shadowDrawable"/>
        <attr format="dimension" name="shadowWidth"/>
        <attr format="boolean" name="closeOnTapEnabled"/>
        <attr format="boolean" name="openOnTapEnabled"/>
        <attr format="dimension" name="offsetWidth"/>
        <attr format="enum" name="stickTo">
            <enum name="auto" value="0"/>
            <enum name="right" value="-1"/>
            <enum name="left" value="-2"/>
            <enum name="middle" value="-3"/>
            <enum name="top" value="-4"/>
            <enum name="bottom" value="-5"/>
        </attr>
    </declare-styleable>
</resources>